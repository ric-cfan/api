package org.serratec.dto;

public class LivroQntPorAutorDTO {
	private Long id;

	private String nome;

	private Long qntLivros;


	public LivroQntPorAutorDTO(Long id, String nome, Long qntLivros) {
		super();
		this.id = id;
		this.nome = nome;
		this.qntLivros = qntLivros;
	}

	public LivroQntPorAutorDTO() {
		super();
	}

	public Long getId() {
		return id;
	}

	public void setId(Long id) {
		this.id = id;
	}

	public String getNome() {
		return nome;
	}

	public void setNome(String nome) {
		this.nome = nome;
	}

	public Long getQntLivros() {
		return qntLivros;
	}

	public void setQntLivros(Long qntLivros) {
		this.qntLivros = qntLivros;
	}

}